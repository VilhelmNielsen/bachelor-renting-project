{
  "name": "my-app",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/VilhelmNielsen/bachelor-renting-project/tree/master/backend"
  },
  "scripts": {
    "start": "node -r dotenv/config src/index.js",
    "debug": "nodemon -e js,graphql -x node --inspect -r dotenv/config src/index.js",
    "format": "prettier --write '**/*.+(js|jsx|md|ts|css|scss|sass|less|graphql|yml|yaml|json)'",
    "playground": "graphql playground",
    "develop": "npm-run-all --parallel debug playground"
  },
  "husky": {
    "hooks": {
      "precommit": "lint-staged"
    }
  },
  "dependencies": {
    "bcrypt": "^3.0.2",
    "cookie-parser": "^1.4.3",
    "dotenv": "^6.2.0",
    "graphql-yoga": "^1.16.7",
    "jsonwebtoken": "^8.4.0",
    "prisma-binding": "^2.2.8"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "eslint": "^5.9.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-graphql": "^3.0.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^8.0.0",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "husky": "^1.2.0",
    "lint-staged": "^8.1.0",
    "nodemon": "^1.18.7",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.15.3",
    "prisma": "^1.22.1"
  }
}
